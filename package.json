  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "isparta": "^3.0.4",
    "jsdom": "^6.3.0",
    "mocha": "^2.3.0",
    "react": "^0.13.3",
