{
  "name": "react-reffect",
  "version": "2.1.1",
  "description": "Create components whose prop changes map to a global side effect with the power of redux and new Context API",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm test && npm run clean && npm run build",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openameba/react-reffect.git"
  },
  "keywords": [
    "react",
    "component",
    "side",
    "effect",
    "thread-safe",
    "renderToNodeStream",
    "renderToNodeStaticStream",
    "context",
    "provider",
    "consumer",
    "redux"
  ],
  "author": "Bin Hou (https://github.com/kouhin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-side-effect/issues"
  },
  "homepage": "https://github.com/gaearon/react-side-effect",
  "contributors": [
    "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
    "Louis DeScioli <louis.descioli@gmail.com>"
  ],
  "dependencies": {
    "exenv": "^1.2.2",
    "hoist-non-react-statics": "^2.5.0",
    "react-broadcast": "^0.7.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-2": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "isparta": "^4.0.0",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.4.3"
